{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/electron/src/main/main.dev.js",
      "stopOnEntry": false,
      "args": [
        "--remote-debugging-port=5858"
      ],
      "cwd": "${workspaceRoot}",
      // this points to the electron task runner
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
      "windows": {
        "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron.cmd"
      },
      "runtimeArgs": [
        "--nolazy"
      ],
      "env": {
        "NODE_ENV": "development"
      },
      "protocol": "inspector",
      "sourceMaps": false
    },
    {
      "name": "Attach",
      "type": "node",
      "protocol": "inspector",
      "request": "attach",
      "port": 5858,
      "sourceMaps": true,
      "localRoot": "${workspaceRoot}",
      "remoteRoot": null
    }
  ],
  "compounds": [
    {
      "name": "Electron: All",
      "configurations": [
        "Launch",
        "Attach"
      ]
    }
  ]
}